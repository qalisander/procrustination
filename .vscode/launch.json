{
    // How to add launch
    // - generate launch
    // - add prebuild task ("build")
    // - change name like "run and build" (to avoid name collision with rust-analyzer)
    // - add arguments
    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in library 'rust_lab'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=rust_lab"
                ],
                "filter": {
                    "name": "rust_lab",
                    "kind": "lib"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        
    ]
}